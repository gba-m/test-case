{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "HealthPayload",
  "type": "object",
  "properties": {
    "status": {
      "type": "string",
      "enum": [
        "ok",
        "degraded",
        "down"
      ]
    },
    "latency": {
      "type": "integer",
      "minimum": 0,
      "maximum": 10000
    }
  },
  "required": [
    "status",
    "latency"
  ],
  "additionalProperties": false
}